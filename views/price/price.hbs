



<div class="row justify-content-center m-0">
    <div class="col-md-6">
              <!-- /.card-header -->
              <div class="card-body ">
                <!-- we are adding the accordion ID so Bootstrap's collapse plugin detects it -->
                <div id="accordion">
                  {{#if project.length}}
                  {{#each project}}
                  <div class="card-t ">
                    <div class="card-header d-flex align-items-center">
                      <h4 class="card-title w-100">
                        <a class="d-block black  collapsed" data-toggle="collapse" href="#a{{id}}" aria-expanded="false">
                        {{name}}
                      </h4>
                        </a>
                        <h3 class="price">{{price}}</h3>
                      </h4>
                      {{#iftr @root.isadmin @root.ismanager}}
                      <a href="/price/edit/{{id}}"><svg stroke="currentColor" fill="none" stroke-width="2" viewBox="0 0 24 24" stroke-linecap="round" stroke-linejoin="round" height="1em" width="1em" xmlns="http://www.w3.org/2000/svg"><path d="M11 4H4a2 2 0 0 0-2 2v14a2 2 0 0 0 2 2h14a2 2 0 0 0 2-2v-7"></path><path d="M18.5 2.5a2.121 2.121 0 0 1 3 3L12 15l-4 1 1-4 9.5-9.5z"></path></svg></a>
                      <a href="/price/delete/{{id}}" ><svg class="delete" stroke="currentColor" fill="currentColor" stroke-width="0" viewBox="0 0 448 512" height="1em" width="1em" xmlns="http://www.w3.org/2000/svg"><path d="M268 416h24a12 12 0 0 0 12-12V188a12 12 0 0 0-12-12h-24a12 12 0 0 0-12 12v216a12 12 0 0 0 12 12zM432 80h-82.41l-34-56.7A48 48 0 0 0 274.41 0H173.59a48 48 0 0 0-41.16 23.3L98.41 80H16A16 16 0 0 0 0 96v16a16 16 0 0 0 16 16h16v336a48 48 0 0 0 48 48h288a48 48 0 0 0 48-48V128h16a16 16 0 0 0 16-16V96a16 16 0 0 0-16-16zM171.84 50.91A6 6 0 0 1 177 48h94a6 6 0 0 1 5.15 2.91L293.61 80H154.39zM368 464H80V128h288zm-212-48h24a12 12 0 0 0 12-12V188a12 12 0 0 0-12-12h-24a12 12 0 0 0-12 12v216a12 12 0 0 0 12 12z"></path></svg></a>
                      {{/iftr}}
                      <!-- Modal -->
                       <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true"/>
                      
                       </div>
                    </div>
                    <div id="a{{id}}" class="collapse" data-parent="#accordion" />
                      <div class="card-body">
                        <ol class="price-list">
                          {{#each allInfo}}
                          <li>{{text}}</li> 
                          {{/each}}
                        </ol>
                      </div>
                    </div>
                  </div>
                  {{/each}}
                  {{else}}
                  {{/if}}
            
                </div>
              </div>
            </div>
          </div>



